datasource propertyproDb {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator clientPropertypro {
  provider = "prisma-client-js"
  output   = "../../node_modules/.prisma/client-propertypro"
}


model Profile {
  id         Int    @id @default(autoincrement())
  userId     Int
  bio        String?
  location   String?
  createdAt  DateTime @default(now())
}
